class = Alnum
      | Alpha
      | Digit
      | Xdigit
      | Space
      | Blank
      | Upper
      | Lower
      | Graph
      | Ctrl
      | Print
      ;


glob = KleeneStar(glob* p)
     | KleenePlus(glob* p)
     | Optional(glob* p)
     | ExactMatch(glob* p)
     | NegateMatch(glob* p)
     | Range(glob l, glob r)
     | Sequence(glob* s)
     | Char(int c)
     | Num(int c)
     ;
